# Install gdown to allow downloading pin file from Google Drive.  
pip install gdown

# Download pin file from https://drive.google.com/file/d/16MGtjxYf4aZ7Ziy-zNKkbYu8pbCnGc6i/view?usp=share_link
cd pins
gdown 16MGtjxYf4aZ7Ziy-zNKkbYu8pbCnGc6i
unzip pinplay-drdebug-3.5-pin-3.5-97503-gac534ca30-gcc-linux.zip
cd -

# Clone the Scarab repo
git clone git@github.com:hpsresearchgroup/scarab.git
